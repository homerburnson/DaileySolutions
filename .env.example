# LLM Configs
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx # Mock OpenAI API key for accessing OpenAI services
OPEN_AI_MODEL=gpt-4o-mini                                 # The OpenAI model to use (e.g., gpt-3.5-turbo, gpt-4)
MAX_TOKENS=1000                                           # Maximum number of tokens for OpenAI responses
TEMPERATURE=0.5                                           # Controls randomness in responses (0 = deterministic, 1 = creative)

# AUTH0 Configs
AUTH0_SECRET=your-auth0-secret                            # Secret used for Auth0 session management
AUTH0_CLIENT_ID=your-auth0-client-id                      # Client ID for your Auth0 application
AUTH0_DOMAIN=your-auth0-domain.auth0.com                  # Auth0 domain (e.g., your-app.auth0.com)

# Network configurations
PORT=3000                                                 # Port on which the server will run
NODE_ENV=development                                      # Environment mode (e.g., development, production)
BASE_URL=https://localhost                           # Base URL for the application (used for redirects and API calls)

# Secrets
SESSION_SECRET=your-session-secret                        # Secret used for session management (e.g., cookies)

# User Configs
NAME=Kermit Grundle                                      # Name of the user (used in system prompts)
TITLE=Human                                              # User's title or role
BRAND=Hedge Fund Damager                                 # User's brand or expertise
LOCATION=Bath                                            # User's location
PROMPT_1=What is a hedge fund damager?                   # Standard prompt 1
PROMPT_2=Who are you?                                    # Standard prompt 2
PROMPT_3=Do you like Bath.                               # Standard prompt 3

# DB Configs
DBSECRET=your-qdrant-api-key                              # API key for authenticating with Qdrant
CLUSTERURL=http://localhost:6333                          # URL of the Qdrant cluster (e.g., local or cloud instance)
QDRANT_COLLECTION_NAME=document_embeddings                # Name of the Qdrant collection for storing embeddings